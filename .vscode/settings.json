{
    "i18n-ally.localesPaths": "tightbeam-desktop/src/lang",
    "i18n-ally.enabledFrameworks": [
        "react-i18next"
    ],
    "i18n-ally.enabledParsers": [
        "json"
    ],
    "i18n-ally.keystyle": "nested",
    "cssVariables.lookupFiles": [
        "**/*.css",
        "**/*.scss",
        "**/*.sass",
        "**/*.less",
        "tightbeam-desktop/node_modules/@mantine/core/styles.css"
    ]
}